/// {
///     "description": "single and double quoted attributes",
///     "result": "<div class=\"b-hello\" title=\"nop\">Hello</div><div class=\"b-hello\" title=\"nop\">Hello</div>"
/// }

match / {
    <div class="b-hello" title="{ .username }">Hello</div>
    <div class='b-hello' title='{ .username }'>Hello</div>
}

