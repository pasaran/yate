/// {
///     description: 'inline object as a function param',
///     data: {
///         item: [
///             { text: 'First' },
///             { text: 'Second' },
///             { text: 'Third' }
///         ]
///     },
///     known: true,
///     result: '<h1>Hello, World</h1><ul><li>First</li><li>Second</li><li>Third</li></ul>'
/// }

match / {
    foo({
        "title": "Hello, World"
        "item": [
            for .item {
                .text
            }
        ]
    })
}

func foo(nodeset a) {
    <h1>{ a.title }</h1>
    <ul>
        for a.item {
            <li>{ . }</li>
        }
    </ul>
}

